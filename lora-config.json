{
    "global": {
        "mainCartridge": "app_training",
        "cartridgePath": [
            "app_training",
            "ui_lora",
            "app_catalog",
            "app_base",
            "app_cart",
            "app_account",
            "app_checkout",
            "app_demo",
            "app_bazaarvoice"
        ],
        "sass": true,
        "es6": true,
        "lintOnSave": true,
        "sourceMaps": true,
        "jspm": true,
        "handlebars": true,
        "tests": true,
        "move": true,
        "lintJs": true,
        "lintSass": true,
        "dwdav": true,
        "inlineSVG": true,
        "imagemin": true,
        "styleguide": true
    },
    "plugins": {
        "eslint": {
            "maxWarnings": 20
        },
        "stylelint": {
            "ignoreFiles": false
        },
        "dwdav": {
            "serverFilesExtension": ["isml", "json", "properties", "xml", "js", "ds"],
            "clientFilesExtension": ["js", "css", "json", "png", "jpg", "gif", "map", "ttf", "woff", "woff2"]
        },
        "styleguide": {
            "logoSrc": "data:image/png;base64,"
        },
        "lighthouse": {
            "categories": {
                "performance": {
                    "minScore": 100
                },
                "pwa": {
                    "minScore": 100
                },
                "seo": {
                    "minScore": 100
                },
                "accessibility": {
                    "minScore": 100
                },
                "best-practices": {
                    "minScore": 100
                }
            }
        }
    },
    "paths": {
        "sourceRoot": "cartridges/app_training/cartridge/static/default",
        "distRoot": "cartridges/app_training/cartridge/static/default/dist",
        "modules": {
            "images": {
                "root": "images",
                "sources": [
                    "**/*.{gif,jpg,png,webp}"
                ],
                "dist": "images"
            },
            "server-scripts": [
                {
                    "sourceRoot": "cartridges/app_training",
                    "root": "cartridge",
                    "sources": [
                        "scripts/**/*.{js,ds}",
                        "controllers/**/*.js"
                    ]
                }
            ]
        }
    }
}